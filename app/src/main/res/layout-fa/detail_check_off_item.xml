<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_margin="8dp"
    android:clickable="true"
    android:focusable="true"
    card_view:cardCornerRadius="10dp"
    card_view:cardElevation="10dp"
    card_view:contentPadding="5dp"
    tools:ignore="MissingPrefix"
    card_view:cardBackgroundColor="@color/background"
    android:id="@+id/cardview">

    <LinearLayout
        android:id="@+id/det_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:baselineAligned="false"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >

                <TextView
                    android:id="@+id/officename_"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:gravity="right"
                    android:maxLines="1"
                    android:text="@string/officename_"
                    android:textColor="@color/text"
                    android:textSize="16sp"
                    android:fontFamily="@font/fontfamily"/>

                <TextView
                    android:id="@+id/officename"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_toLeftOf="@+id/officename_"
                    android:layout_marginBottom="5dp"
                    android:gravity="right"
                    android:maxLines="1"
                    android:text="@string/officename"
                    android:textColor="@color/text"
                    android:textSize="16sp"
                    android:fontFamily="@font/fontfamily"/>


            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >

                <TextView
                    android:id="@+id/insurance_"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginBottom="5dp"
                    android:gravity="right"
                    android:maxLines="1"
                    android:text="@string/insurance_"
                    android:textColor="@color/text"
                    android:textSize="16sp"
                    android:fontFamily="@font/fontfamily"/>

                <TextView
                    android:id="@+id/insurance"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_toLeftOf="@+id/insurance_"
                    android:layout_marginBottom="5dp"
                    android:gravity="right"
                    android:maxLines="1"
                    android:text="@string/insurance"
                    android:textColor="@color/text"
                    android:textSize="16sp"
                    android:fontFamily="@font/fontfamily"/>


            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >

                <TextView
                    android:id="@+id/economic_"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:gravity="right"
                    android:maxLines="1"
                    android:text="@string/economic_"
                    android:textColor="@color/text"
                    android:textSize="16sp"
                    android:fontFamily="@font/fontfamily"/>

                <TextView
                    android:id="@+id/economic"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_toLeftOf="@+id/economic_"
                    android:layout_marginBottom="5dp"
                    android:gravity="right"
                    android:maxLines="1"
                    android:text="@string/economic"
                    android:textColor="@color/text"
                    android:textSize="16sp"
                    android:fontFamily="@font/fontfamily"/>



            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >

                <TextView
                    android:id="@+id/phone_"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:gravity="right"
                    android:maxLines="1"
                    android:text="@string/phone_"
                    android:textColor="@color/text"
                    android:textSize="16sp"
                    android:fontFamily="@font/fontfamily"/>

                <TextView
                    android:id="@+id/phone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_toLeftOf="@+id/phone_"
                    android:layout_marginBottom="5dp"
                    android:gravity="right"
                    android:maxLines="1"
                    android:text="@string/phone"
                    android:textColor="@color/text"
                    android:textSize="16sp"
                    android:fontFamily="@font/fontfamily"/>


            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >

                <TextView
                    android:id="@+id/officetype_"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:gravity="right"
                    android:maxLines="1"
                    android:text="@string/officetype_"
                    android:textColor="@color/text"
                    android:textSize="16sp"
                    android:fontFamily="@font/fontfamily"/>

                <TextView
                    android:id="@+id/officetype"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_toLeftOf="@+id/officetype_"
                    android:layout_marginBottom="5dp"
                    android:gravity="right"
                    android:maxLines="1"
                    android:text="@string/officetype"
                    android:textColor="@color/text"
                    android:textSize="16sp"
                    android:fontFamily="@font/fontfamily"/>


            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >

                <TextView
                    android:id="@+id/address_"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:gravity="right"
                    android:maxLines="1"
                    android:text="@string/address_"
                    android:textColor="@color/text"
                    android:textSize="16sp"
                    android:fontFamily="@font/fontfamily"/>

                <TextView
                    android:id="@+id/address"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="5dp"
                    android:layout_toLeftOf="@+id/address_"
                    android:layout_marginBottom="5dp"
                    android:gravity="right"
                    android:maxLines="4"
                    android:lineSpacingExtra="10dp"
                    android:text="@string/address"
                    android:textColor="@color/text"
                    android:textSize="16sp"
                    android:fontFamily="@font/fontfamily"/>


            </RelativeLayout>


        </LinearLayout>

    </LinearLayout>
</androidx.cardview.widget.CardView>
